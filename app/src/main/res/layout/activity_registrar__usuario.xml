<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.rafa.games.Registrar_Usuario">

    <EditText
        android:id="@+id/usuario_registro"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView13"
        android:layout_centerHorizontal="true"
        android:ems="10"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/correo_registro"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/telefono_registro"
        android:layout_alignLeft="@+id/usuario_registro"
        android:layout_alignStart="@+id/usuario_registro"
        android:layout_marginBottom="34dp"
        android:ems="10"
        android:inputType="textEmailAddress" />

    <EditText
        android:id="@+id/telefono_registro"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/correo_registro"
        android:layout_alignRight="@+id/correo_registro"
        android:layout_centerVertical="true"
        android:ems="10"
        android:inputType="phone" />

    <EditText
        android:id="@+id/contraseña_registro"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/telefono_registro"
        android:layout_alignStart="@+id/telefono_registro"
        android:layout_below="@+id/telefono_registro"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textPassword" />

    <EditText
        android:id="@+id/contraseñar_registro"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/contraseña_registro"
        android:layout_alignStart="@+id/contraseña_registro"
        android:layout_below="@+id/contraseña_registro"
        android:layout_marginTop="27dp"
        android:ems="10"
        android:inputType="textPassword" />

    <Button
        android:id="@+id/registrarmebtn_registro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/contraseñar_registro"
        android:layout_alignStart="@+id/contraseñar_registro"
        android:layout_below="@+id/contraseñar_registro"
        android:layout_marginTop="16dp"
        android:text="Registrame" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/usuario_registro"
        android:layout_alignStart="@+id/usuario_registro"
        android:layout_below="@+id/textView19"
        android:layout_marginTop="24dp"
        android:text="Usuario" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/correo_registro"
        android:layout_alignStart="@+id/correo_registro"
        android:layout_below="@+id/usuario_registro"
        android:layout_marginTop="11dp"
        android:text="Correo electrónico" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/telefono_registro"
        android:layout_alignLeft="@+id/telefono_registro"
        android:layout_alignStart="@+id/telefono_registro"
        android:text="Teléfono" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/contraseña_registro"
        android:layout_alignStart="@+id/contraseña_registro"
        android:layout_below="@+id/telefono_registro"
        android:layout_marginTop="8dp"
        android:text="Contraseña" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/contraseñar_registro"
        android:layout_alignStart="@+id/contraseñar_registro"
        android:layout_below="@+id/contraseña_registro"
        android:layout_marginTop="7dp"
        android:text="Repetir contraseña" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:text="Registro de usuario"
        android:textColor="#000000"
        android:textSize="20sp" />
</RelativeLayout>
